{
	"name": "@lesjoursfr/html-to-pdf",
	"version": "1.0.2",
	"description": "A library to generate PDF from HTML.",
	"license": "MIT",
	"repository": "lesjoursfr/html-to-pdf",
	"homepage": "https://github.com/lesjoursfr/html-to-pdf#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/html-to-pdf/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=v16.13.1"
	},
	"keywords": [
		"pdf",
		"generator",
		"html"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*",
		"templates/**/*"
	],
	"scripts": {
		"build": "npx tsc && cp src/electron.js lib/electron.js",
		"test": "ava"
	},
	"dependencies": {
		"electron": "^16.0.4",
		"yargs": "^17.3.0"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/node": "^16.11.12",
		"@types/yargs": "^17.0.7",
		"@typescript-eslint/eslint-plugin": "^5.6.0",
		"@typescript-eslint/parser": "^5.6.0",
		"ava": "^4.0.0-rc.1",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.2.0",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.2"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"packageManager": "yarn@3.1.1"
}
