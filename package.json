{
	"name": "@lesjoursfr/html-to-pdf",
	"version": "1.0.5",
	"description": "A library to generate PDF from HTML.",
	"license": "MIT",
	"repository": "lesjoursfr/html-to-pdf",
	"homepage": "https://github.com/lesjoursfr/html-to-pdf#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/html-to-pdf/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=v16.14.2"
	},
	"keywords": [
		"pdf",
		"generator",
		"html"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*",
		"templates/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"build": "npx tsc && cp src/electron.js lib/electron.js",
		"lint": "npx eslint --ext .ts --fix src/",
		"test": "ava"
	},
	"dependencies": {
		"electron": "^18.0.4",
		"yargs": "^17.4.1"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/node": "^16.11.27",
		"@types/yargs": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"ava": "^4.2.0",
		"eslint": "^8.13.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"packageManager": "yarn@3.2.0"
}
